public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
    //code here
if(isPassed = passedExam && passedAssignments){
    passed = true;
}
else if (isPassed = passedProject && passedAssignments){
     passed = true;
}

else if (isPassed = passedExam && passedProject){
    passed = true;
}
return passed

}