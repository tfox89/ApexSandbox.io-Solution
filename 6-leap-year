public Boolean isLeapYear(Integer year) {
    //code here
    if(Math.mod year,4 !=0){
        return false;
    }
    else if ((Math.mod year,4) && (year,100 != 0)){
        return false;
    }
    else if ((Math.mod year,4) && (year,100) && (year,400 !=0)){
        return false;
    }
    else{
        return true;
    }
}
